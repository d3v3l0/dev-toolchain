[core]
    editor = emacs
    autocrlf = input
    excludesfile = /Users/wesm/.gitexcludes
[user]
    name = Wes McKinney
    email = wesm+git@apache.org
[alias]
    st = status
    stat = status
    co = checkout
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)[%an]%Creset' --abbrev-commit --date=relative
    b = branch
    ba = branch -a
    c = commit -v
    d = diff
    l = pull
    p = push
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[color]
    diff = auto
    status = true
[branch]
    autosetupmerge = true
[merge]
    log = true
    tool = tkdiff
    conflictstyle = diff3
[mergetool]
    log = true
    prompt = false
[push]
    default = tracking
